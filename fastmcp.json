{
  "$schema": "https://gofastmcp.com/public/schemas/fastmcp.json/v1.json",
  "source": {
    "type": "filesystem",
    "path": "crawler_mcp/server.py",
    "entrypoint": "mcp"
  },
  "environment": {
    "type": "uv",
    "python": ">=3.11",
    "project": "."
  },
  "deployment": {
    "transport": "http",
    "host": "0.0.0.0",
    "port": 8010,
    "path": "/mcp/",
    "log_level": "INFO",
    "env": {
      "PRODUCTION": "true",
      "SERVER_PORT": "8010"
    }
  }
}
